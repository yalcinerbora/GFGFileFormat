global proc string GFGReDeploy (string $type)
{
	string $gfgOptsIDest = "C:/Program Files/Autodesk/Maya2016/scripts/others/GFGOptsImport.mel";
	string $gfgOptsISrc = "D:/GitHub/GFGFileFormat/Source/Autodesk Maya/GFGOptsImport.mel";
	
	string $gfgOptsEDest = "C:/Program Files/Autodesk/Maya2016/scripts/others/GFGOptsExport.mel";
	string $gfgOptsESrc = "C:/Program Files/Autodesk/Maya2016/scripts/others/GFGOptsExport.mel";
	
	string $gfgReDeployDest = "C:/Program Files/Autodesk/Maya2016/scripts/others/GFGReDeploy.mel";
	string $gfgReDeploySrc = "D:/GitHub/GFGFileFormat/Source/Autodesk Maya/GFGReDeploy.mel";
	
	string $gfgErrWindowDest = "C:/Program Files/Autodesk/Maya2016/scripts/others/GFGErrorWindow.mel";
	string $gfgErrWindowSrc = "D:/GitHub/GFGFileFormat/Source/Autodesk Maya/GFGErrorWindow.mel";

	sysFile -copy $gfgOptsEDest $gfgOptsESrc;
	sysFile -copy $gfgOptsIDest $gfgOptsISrc;
	sysFile -copy $gfgErrWindowDest $gfgErrWindowSrc;
	sysFile -copy $gfgReDeployDest $gfgReDeploySrc;
	source "GFGOptsImport.mel";
	source "GFGOptsExport.mel";
	source "GFGErrorWindow.mel";
	source "GFGReDeploy.mel";
 
	if($type == "debug")
	{
		//DEBUG
		string $destMLL = "C:/Program Files/Autodesk/Maya2016/bin/plug-ins/GFGMaya.mll";
		string $srcMLL = "D:/GitHub/GFGFileFormat/Bin/Windows/Debug/GFGMaya.mll";

		string $destPDB = "C:/Program Files/Autodesk/Maya2016/bin/plug-ins/GFGMaya.pdb";
		string $srcPDB = "D:/GitHub/GFGFileFormat/Bin/Windows/Debug/GFGMaya.pdb";
		
		unloadPlugin "GFGMaya.mll";
		sysFile -copy $destMLL $srcMLL;
		sysFile -copy $destPDB $srcPDB;
		loadPlugin "GFGMaya.mll";
	}
	else if($type == "release")
	{
		//RELEASE
		string $destMLL = "C:/Program Files/Autodesk/Maya2016/bin/plug-ins/GFGMaya.mll";
		string $srcMLL = "D:/GitHub/GFGFileFormat/Bin/Windows/Release/GFGMaya.mll";

		unloadPlugin "GFGMaya.mll";
		sysFile -copy $destMLL $srcMLL;
		loadPlugin "GFGMaya.mll";
	}
	else
	{
		return "ERROR: Type is not debug or release";
	}
	
	return "OK";
}
